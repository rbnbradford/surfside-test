RECENTLY_SEEN_STORE_VALKEY_HOST=valkey
RECENTLY_SEEN_STORE_VALKEY_PORT=6379

# required with IMPRESSION_WRITER_METHOD=clickhouse
IMPRESSION_WRITER_CLICKHOUSE_URL=http://clickhouse:8123
IMPRESSION_WRITER_CLICKHOUSE_DB=impressions_db
IMPRESSION_WRITER_CLICKHOUSE_USER=default
IMPRESSION_WRITER_CLICKHOUSE_PASS=password