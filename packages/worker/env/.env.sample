APP_ENV=development
NODE_ENV=development

KAFKA_CONSUMER_CLIENT_ID=surfside-test-worker
KAFKA_CONSUMER_GROUP_ID=surfside-test-worker
KAFKA_CONSUMER_BROKERS=kafka:9092
KAFKA_CONSUMER_TOPIC=impressions

# valkey | in-memory
RECENTLY_SEEN_STORE_METHOD=valkey
RECENTLY_SEEN_STORE_TTL_SECONDS=86400
